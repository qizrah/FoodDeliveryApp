@page
@model FoodDelivery.Pages.Admin.FoodTypes.IndexModel

@{
    ViewData["Title"] = "Food Types";
}



<div class="border backgroundWhite container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Food Type List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-page="Upsert" asp-route-id="0" class="btn btn-primary">
                <i class="fas fa-plus"></i>&nbsp;Create New
            </a>
        </div>
    </div>
    <br /><br />
    <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>
                    FoodType Name
                </th>
                <th></th>
            </tr>
        </thead>

    </table>
</div>
@section Scripts {
    <script src="~/js/foodtype.js"></script>
}

